{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook",
      "methods": ["POST"]
    },
    {
      "src": "/webhook",
      "dest": "/api/webhook",
      "methods": ["POST"]
    }
  ]
}
